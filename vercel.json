{
  "version":2,
  "builds":[
      {"src":"bd/**","use":"@vercel/python"},
      {"src":"pbc/**","use":"@vercel/static"}
  ],
  "functions": { "bd/**/*.py": { "runtime": "@vercel/python" } },
  "routes":[
    {"src":"/","dest":"h.html"},
    {"src":"/b.py/(.*)","dest":"bd/b.py"},
    {"src":"/p.py/(.*)","dest":"bd/p.py"},
    {"src":"/d.py/(.*)","dest":"bd/d.py"}
  ]
}
